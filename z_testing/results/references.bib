@article{Kailasanathan_2025,
  title = {Heart disease prediction with a feature-sensitized interpretable framework for the Internet of Medical Things sensors.},
  author = {Nallakaruppan Kailasanathan and Gangadevi Ezhilarasan and Shitharth Selvarajan and Rajesh Kumar Dhanaraj and Dragan Pamucar and Nathan Shankar},
  journal = {Frontiers in digital health},
  year = {2025},
