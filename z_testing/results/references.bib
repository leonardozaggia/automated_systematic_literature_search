@article{Lawton_2025,
  title = {Artificial intelligence in paediatric respiratory medicine.},
  author = {Adam Lawton and Dominic Hughes},
  journal = {Paediatric respiratory reviews},
  year = {2025},
  doi = {10.1016/j.prrv.2025.10.001},
  url = {https://pubmed.ncbi.nlm.nih.gov/41107075/},
  pmid = {41107075},
  abstract = {Proponents of artificial intelligence (AI) believe that it will revolutionise the modern world, affecting how healthcare is delivered and improve both the clinical care we provide and the ease with which we perform our work. In this paper we explain what is meant by 'artificial intelligence' and explore how this technology has been implemented, or might be implemented, with respect to paediatric respiratory medicine. We review the current literature on how AI has been used to improve diagnostics - including examples in radiology, primary ciliary dyskinesia (PCD) diagnostics, sleep medicine, and pulmonary function tests. We also review how AI has been applied to therapeutics and drug discovery, how it will impact evidence-based medicine and literature review, and how clinician support tools will assist us in our work.},
}

@article{Chatha_2025,
  title = {Shaping the Clinician II: Artificial Intelligence-Taught Endodontic Skills.},
  author = {Nauman R Chatha and Kisa Iqbal and Fahad Zaman and Kelsey Hebert},
  journal = {Dental clinics of North America},
  year = {2025},
  volume = {69},
  number = {4},
  pages = {585-602},
  doi = {10.1016/j.cden.2025.05.011},
  url = {https://pubmed.ncbi.nlm.nih.gov/41106912/},
  pmid = {41106912},
  abstract = {This article explores how artificial intelligence (AI) is transforming endodontic education and clinical practice. It highlights the impact of AI tools-such as machine learning, neural networks, computer vision, virtual reality, and augmented reality-in enhancing diagnostic accuracy, procedural skills, and personalized learning. Traditional training methods relying on lectures and limited hands-on practice are contrasted with AI-driven approaches offering standardized assessment, real-time feedback, and immersive simulations. The article addresses challenges related to ethical considerations, data privacy, and interdisciplinary collaboration. Through case studies and pilot programs, it demonstrates how AI can complement clinical expertise and revolutionize the training of dental professionals.},
}

@article{McCarthy_2025,
  title = {Artificial Intelligence and Ethics in Endodontics.},
  author = {Michael McCarthy and Nanette Elster},
  journal = {Dental clinics of North America},
  year = {2025},
  volume = {69},
  number = {4},
  pages = {555-562},
  doi = {10.1016/j.cden.2025.05.005},
  url = {https://pubmed.ncbi.nlm.nih.gov/41106910/},
  pmid = {41106910},
  abstract = {AI in endodontics is driven by machine learning techniques and robotics. This chapter identifies ethical issues raised by AI in endodontics and describes a framework for evaluating its ethically responsible implementation as a tool that should continue to prioritize the best interest of patients. Beyond its clinical implementation, the chapter considers how AI technology can be utilized for managing the business aspects within endodontics in a way that keeps the patient at the center. Finally, the chapter concludes by offering a framework for considering the epistemic and normative questions at the intersection of AI and endodontics.},
}

